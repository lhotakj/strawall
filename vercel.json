{
  "version": 2,
  "regions": [
    "fra1"
  ],
  "env": {
    "environment": "VERCEL",
    "redis": "localhost"
  },
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "postbuild": "apt-get install -y ./install/wkhtmltox_0.12.6.1-2.jammy_amd64.deb"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/app.py"
    },
    {
      "src": "/stats.png",
      "dest": "/app.py"
    }
  ]
}
