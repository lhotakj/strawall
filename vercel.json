{
  "version": 2,
  "regions": [
    "fra1"
  ],
  "env": {
    "environment": "VERCEL",
    "redis": "localhost"
  },
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/app.py"
    },
    {
      "src": "/stats.png",
      "dest": "/app.py"
    }
  ]
}
