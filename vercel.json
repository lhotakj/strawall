{
  "version": 2,
  "regions": [
    "fra1"
  ],
  "env": {
    "environment": "VERCEL",
    "redis": "localhost"
  },
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/node",
      "env": {
        "INSTALL_DEB": "apt-get install -y /install/wkhtmltox_0.12.6.1-2.jammy_amd64.deb"
      }
    },    
    {
      "src": "index.py",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}

